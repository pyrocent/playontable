{
    "redirects": [
        {
            "source": "/((?!api).*)",
            "has": [
                {
                    "type": "host",
                    "value": "api.playontable.com"
                }
            ],
            "destination": "playontable.com",
            "statusCode": 403
        },
        {
            "source": "/api/(.*)",
            "has": [
                {
                    "type": "host",
                    "value": "playontable.com"
                }
            ],
            "destination": "playontable.com",
            "statusCode": 403
        }
    ]
}